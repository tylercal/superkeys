{
  "manifest_version": 2,

  "name": "Super Keys",
  "description": "Configure keyboard shortcuts to perform JavaScript actions on sites.",
  "version": "1.0.2",
  "minimum_chrome_version": "85",

  "options_page": "src/options.html",

  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },


  "permissions": [
    "storage",
    "tabs",
    "webNavigation",
    "<all_urls>"
  ],

  "web_accessible_resources": [
    "src/help.html"
  ],

  "background": {
    "scripts": ["src/background.js"],
    "persistent": false
  }
}